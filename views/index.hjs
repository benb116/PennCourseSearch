<!doctype html>
<html>
	<head>
		<title>PennCourseScheduler</title>
		<meta charset="UTF-8">
		<link rel="stylesheet" type="text/css" href="/css/index.css">
        <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
        <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
        <script type="text/javascript" src="/js/html2canvas.js"></script>
        <script type="text/javascript" src="/js/main.js"></script>
	</head>
    <body>
        <div id="Top">
    	    <h1>{{ title }}</h1>
    	    <span>{{ subtitle }}</span>
            <form onkeypress="return event.keyCode != 13;">
    			<select id="searchSelect">
                    <option selected="selected" value="courseIDSearch">Course ID</option>
                    <option value="keywordSearch">Keywords</option>
                    <option value="instSearch">Instructor</option>
                </select>
                <input id="CSearch" type="text" name="courseSearch" autocomplete="off" placeholder="Search for courses...">
                <span style="margin-left:1em;" class="OpenSec">&nbsp&nbsp&nbsp&nbsp&nbsp</span><span> = Open </span><span class="ClosedSec">&nbsp&nbsp&nbsp&nbsp&nbsp</span><span> = Closed </span><span id="LoadingInfo" style="opacity:0;">Loading...</span>
    		</form>
            <div id="MenuButtons">
        		<span>Toggle course titles</span><span>Clear All</span><span>Download Schedule</span><span>Recolor</span>
            </div>
		</div>
        <div id="BottomResults">
        	<div id="CourseList"></div>
        	<div id="SectionCol">
        		<div id="SectionList"></div>
        		<div id="SectionInfo"></div>
        	</div>
        	<div id="InfoPanel">
                <div id="SchedGraph">
            		<div id="TimeCol" style="position:relative;"></div>
                    <div id="Schedule" style="position:relative;"></div>
                </div>

        	</div>
        </div>
    </body>
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-49014722-3', 'auto');
        ga('send', 'pageview');

    </script>
</html>