<!doctype html>
<html>
	<head>
		<title>PennCourseScheduler</title>
		<meta charset="UTF-8">
		<link rel="stylesheet" type="text/css" href="/css/index.css">
		<link rel='stylesheet' href='http://fonts.googleapis.com/css?family=PT+Sans|Istok+Web|Sniglet|Open+Sans'>
        <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
        <script type="text/javascript" src="/js/main.js"></script>
	</head>
    <body>
        <div id="Top">
	      <h1>{{ title }}</h1>
	      <span>{{ subtitle }}</span>
	      <form onkeypress="return event.keyCode != 13;">
				<select id="TermSelect" style="margin-right:1em;">
                    <option value="2014C">2014 Fall</option>
                    <option selected="selected" value="2015A">2015 Spring</option>
                </select>
                <input id="CSearch" type="text" name="courseSearch" autocomplete="off" placeholder="Search for courses...">
                <span style="margin-left:1em;" class="OpenSec">&nbsp&nbsp&nbsp&nbsp&nbsp</span><span> = Open </span><span class="ClosedSec">&nbsp&nbsp&nbsp&nbsp&nbsp</span><span> = Closed </span><span id="LoadingInfo" style="opacity:0;">Loading...</span>
			</form>
			
		</div>
        <div id="BottomResults">
        	<div id="CourseList"></div>
        	<div id="SectionCol">
        		<div id="SectionList"></div>
        		<div id="SectionInfo"></div>
        	</div>
        	<div id="InfoPanel">
        		<div id="TimeCol" style="position:relative;"></div>
                <div id="Schedule" style="position:relative;"></div>
                <span>Toggle course titles</span><span>Clear All</span>

        	</div>
        </div>
    </body>
</html>